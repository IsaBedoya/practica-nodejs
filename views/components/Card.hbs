    <p class="card-title">{{url.origin}}</p>
    <p class="card-text">{{url.shortURL}}</p>
    <a href="/eliminar/{{url._id}}" class="btn btn-danger">Eliminar</a>
    <a href="/editar/{{url._id}}" class="btn btn-warning">Editar</a>
